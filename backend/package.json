{
  "name": "sse-backend",
  "version": "0.1.0",
  "description": "NestJS SSE backend with transactional outbox and Redis Streams",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "migration:run": "ts-node -r tsconfig-paths/register src/migrations/run.ts",
    "seed": "ts-node -r tsconfig-paths/register scripts/seed.ts",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^7.3.1",
    "@prisma/client": "5.19.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "ioredis": "5.4.1",
    "pg": "8.12.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.7",
    "rxjs": "7.8.1",
    "swagger-ui-express": "^5.0.1",
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "10.2.1",
    "@nestjs/testing": "10.3.10",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@types/supertest": "2.0.16",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "prisma": "5.19.0",
    "supertest": "6.3.4",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  }
}
